* text=auto

# CRLF breaks shebang for Python.
*.py text eol=lf
*.sh text eol=lf

# Others.
.gitignore text eol=lf
.gitattributes text eol=lf
*.c text eol=lf
*.cc text eol=lf
*.csv text eol=lf
*.h text eol=lf
*.ini text eol=lf
*.json text eol=lf
*.md text eol=lf
*.txt text eol=lf
LICENSE text eol=lf
Makefile text eol=lf
Makefile-build text eol=lf

# Known binary file types.
*.png binary
*.jpg binary
